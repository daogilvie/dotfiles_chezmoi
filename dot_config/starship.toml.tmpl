# Starship config! 

# I've decided that I don't like the blank lines
add_newline = false

# Wait a little longer for things (lots of cold starts)
command_timeout = 700

[aws]
disabled = true

[gcloud]
disabled = true

[dotnet]
disabled = true

[package]
disabled = true

[custom.jj]
command = 'jj show -T "change_id.shortest()" --no-patch'
detect_folders = [".jj"]
symbol = 'ðŸ¥‹'
format = 'on $symbol jj@[$output]($style) '
